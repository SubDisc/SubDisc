<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE knimeNode PUBLIC "-//UNIKN//DTD KNIME Node 2.0//EN" "http://www.knime.org/Node.dtd">
<knimeNode icon="./icon.jpg" type="Manipulator">
    <name>Subgroup Filter</name>
    
    <shortDescription>
        Subgroup Filter for Cortana results.
    </shortDescription>

    <fullDescription>
        <intro>Subgroup Filter for Cortana results.</intro>
        
        
        <option name="short name of first option (like in the dialog)">description of first option</option>
        <option name="short name of second option (like in the dialog)">description of second option</option>
    </fullDescription>

    <ports>
        <inPort index="0" name="Data">Input Data (DataTable).</inPort>
        <inPort index="1" name="Condition">A condition selected using a HiLite Filter.</inPort>
        <!-- possibly more input ports here-->
        <outPort index="0" name="Subgroup">Subset of input data for which the HiLite condition holds, ie. the members of a Subgroup.</outPort>
        <!-- possibly more output ports here-->
    </ports>    
</knimeNode>
